<%- include("../partials/head") %> <%- include("../partials/nav") %>
    <div class="container row max">
      <%- include("../partials/sidebar") %>
        <main class="main max relative">
          <%- include("./toolbar") %>
            <div class="body">
              <section>
                <%- include("./tabs") %>

                  <div class="gridlist padding">
                    <% if(data.parties) { %>
                      <% data.parties.map(function each(party, index){ %>
                        <section class="gridcell center-align">
                          <img class="figure" src="/images/sample.png" alt="" />
                          <h3 class="headline">
                            <%= party.party %>
                          </h3>
                          <small class="infoline">
                             2 Candidates
                          </small>
                        </section>
                        <%}) %>
                          <% } else {%>
                            <h1>no party register</h1>
                            <% } %>
                  </div>
              </section>
            </div>
        </main>
    </div>
    <%- include("../partials/foot") %>