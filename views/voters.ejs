<%- include("./partials/head") %> <%- include("./partials/nav") %>
<div class="dash">
  <%- include("./partials/sidebar") %>
  <main class="main">
    <nav class="toolbar row">
      <section class="max">
        <a href="voters/registration" class="button"> + Voter Registration</a>
      </section>
      <section>Actions</section>
    </nav>
    <div class="tablist">
      <% data.voters.map((voter, key) => { %>
      <section class="tabitem" id="<%= voter.id %>">
        <span class="tabicon">
          <img src="/images/sample.png" alt="icon" class="tabimg" />
        </span>
        <span class="tabcontent max">
          <strong>
            <%= voter.first_name %> <%= voter.middle_name %>. <%=
            voter.last_name %>
          </strong>
          <small> <%= voter.DOB %> &middot; Male</small>
        </span>
      </section>
      <%}) %>
    </div>
  </main>
</div>
<%- include("./partials/foot") %>
