<%- include("./partial/head") %>

<body class="column">
    <main class="max center">
        <form action="/parties/registration" method="post" enctype="multipart/form-data" class="box border">
            
            <label for="party"   class="label">Party Name:</label>
            <input type="text" name="party" id="party" class="input">

            <label for="logo" class="label">Logo:</label>
            <input type="file" name="logo" id="logo" class="input">

            <span class="space"></span>
            <button type="submit">Create Party</button>
        </form>
    </main>
</body>
<script type="module" src="/js/parties.js"></script>
</html>