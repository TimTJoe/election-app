<%- include("./partial/head") %>

    <body>
        <div>
            <h4>total voters: <%= data.total.voters %></h4>
            <h4>total votes: <%= data.total.votes %></h4>
        </div>
        <table id="voters">
            <h1>voters</h1>
            <% data.voters.map((voter,index)=> { %>
                <tr>
                    <td>
                        <%= voter.first_name %>
                        <%= voter.middle_name %>
                        <%= voter.last_name %>
                    </td>
                    <td><a href="/votes">Select to Vote</a></td>
                </tr>
            <%})%>
        </table>
    </body>
    <!-- <script type="module" src="/js/dashboard.js"></script> -->
    </html>